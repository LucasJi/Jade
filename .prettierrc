{
  "arrowParens": "avoid",
  "singleQuote": true,
  "trailingComma": "all",
  "tabWidth": 2,
  "endOfLine": "auto",
  "organizeImportsSkipDestructiveCodeActions": true,
  "plugins": [
    "prettier-plugin-organize-imports",
    "prettier-plugin-tailwindcss"
  ],
  "tailwindFunctions": [
    "clsx",
    "cva",
    "cn"
  ],
  "tailwindPreserveDuplicates": true,
  "tailwindPreserveWhitespace": true
}
